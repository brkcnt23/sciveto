{
  "name": "sciveto",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "db:generate": "cd packages/database && pnpm run db:generate",
    "db:push": "cd packages/database && pnpm run db:push",
    "db:migrate": "cd packages/database && pnpm run db:migrate",
    "db:studio": "cd packages/database && pnpm run db:studio",
    "packages:build": "cd packages/database && pnpm run build && cd ../shared-types && pnpm run build",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "^2.5.4",
    "prettier": "^3.0.0"
  },
  "dependencies": {}
}